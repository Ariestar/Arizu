{
  "enabled": true,
  "target_groups": [
    "1057812687"
  ],
  "platform_name": "default",
  "data_sources": {
    "fan_studio": {
      "enabled": true,
      "china_earthquake_warning": true,
      "taiwan_cwa_earthquake": true,
      "china_cenc_earthquake": true,
      "japan_jma_earthquake": true,
      "usgs_earthquake": true,
      "china_weather_alarm": true,
      "china_tsunami": true
    },
    "p2p_earthquake": {
      "enabled": true,
      "japan_jma_eew": true,
      "japan_jma_earthquake": true,
      "japan_jma_tsunami": true
    },
    "wolfx": {
      "enabled": true,
      "japan_jma_eew": true,
      "china_cenc_eew": true,
      "taiwan_cwa_eew": true,
      "japan_jma_earthquake": true,
      "china_cenc_earthquake": true
    },
    "global_quake": {
      "enabled": true,
      "primary_server": true,
      "secondary_server": false
    }
  },
  "earthquake_filters": {
    "intensity_filter": {
      "enabled": true,
      "min_magnitude": 2.0,
      "min_intensity": 4.0
    },
    "scale_filter": {
      "enabled": true,
      "min_magnitude": 2.0,
      "min_scale": 1.0
    },
    "magnitude_only_filter": {
      "enabled": true,
      "min_magnitude": 4.5
    }
  },
  "push_frequency_control": {
    "push_every_n_reports": 1,
    "final_report_always_push": true,
    "ignore_non_final_reports": false
